{
	"name": "Oxipital",
	"type": "OSCQuery",
	"path": "Software",
	"version": "1.0.0",
	"description": "Control Oxipital.",
	"url": "",
	"downloadURL": "",
	"hasInput": true,
	"hasOutput": true,
	"hideDefaultCommands": true,
	"hideDefaultParameters": [],
	"defaults": {
		"serverName": "Unity-Oxipital",
		"output": {
			"remotePort": 12321
		}
	},
	"parameters": {
		"Setup": {
			"type": "Container",
			"Presets": {
				"type": "Container",
				"Preset Name" : {
					"type": "String",
					"default": "OxiPreset"
				},
				"Preset": {
					"type": "Enum"
				},
				"Save New":
				{
					"type": "Trigger"
				},
				"Update Current": {
					"type": "Trigger"
				},
				"Load": {
					"type": "Trigger"
				},
				"Refresh":
				{
					"type": "Trigger"
				},
				"Delete": {
					"type": "Trigger"
				},
				"Async Loading":
				{
					"type": "Boolean",
					"default": true
				},
				"Loaded Values":
				{
					"type": "Container",
					"collapsed":true,
					"Macros": {
						"type": "Container"
					},
					"Orb Groups": {
						"type": "Container"
					},
					"Force Groups": {
						"type": "Container"
					}
				}
			},
			"Num Orb Groups": {
				"type": "Integer",
				"min": 0,
				"max": 8,
				"default": 4
			},
			"Num Force Groups": {
				"type": "Integer",
				"min": 0,
				"max": 8,
				"default": 4
			},
			"Num Macros": {
				"type": "Integer",
				"min": 0,
				"max": 8,
				"default": 4
			}
		},
		"Macros": {
			"type": "Container"
		},
		"Orb Groups": {
			"type": "Container"
		},
		"Force Groups": {
			"type": "Container"
		}
	},
	"scripts": [
		"Oxipital.js"
	],
	"values": {},
	"commands": {
		"Reset": {
			"args": []
		},
		"Set Macro Value": {
			"args": [
				{
					"name": "Macro",
					"type": "Integer",
					"min": 0,
					"max": 8
				},
				{
					"name": "Value",
					"type": "Float",
					"min": 0,
					"max": 1
				}
			]
		},
		"Set Orb Group Value": {
			"args": [
				{
					"name": "Type",
					"type": "Enum",
					"values": {
						"Float": 0,
						"Vector": 1
					}
				},
				{
					"name": "Orb Group",
					"type": "Integer",
					"min": 0,
					"max": 8
				},
				{
					"name": "Float Value",
					"type": "Float",
					"min": 0,
					"max": 1
				},
				{
					"name": "Vector Value",
					"type": "Float",
					"min": 0,
					"max": 1
				}
			]
		}
	}
}